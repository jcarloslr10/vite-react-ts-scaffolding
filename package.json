{
  "name": "vite-react-scaffolding",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=14.20.0",
    "npm": ">=6.14.17"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "lint:scripts": "eslint --ext .ts,.tsx src",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "jest-environment-jsdom": "^29.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@storybook/addon-actions": "^7.0.0-alpha.31",
    "@storybook/addon-essentials": "^7.0.0-alpha.31",
    "@storybook/addon-interactions": "^7.0.0-alpha.31",
    "@storybook/addon-links": "^7.0.0-alpha.31",
    "@storybook/cli": "^7.0.0-alpha.31",
    "@storybook/react-vite": "^7.0.0-alpha.31",
    "@storybook/testing-library": "^0.0.14-next.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-loader": "^8.2.5",
    "cypress": "^10.7.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.2",
    "prettier": "^2.7.1",
    "storybook": "^7.0.0-alpha.31",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testEnvironment": "jest-environment-jsdom"
  }
}
